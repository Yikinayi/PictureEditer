<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="yzx.app.editer">


    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.VIBRATE" />


    <application
        android:name=".App"
        android:allowBackup="true"
        android:fullBackupContent="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="false"
        android:theme="@style/AppTheme">

        <activity
            android:name=".MainActivity"
            android:screenOrientation="portrait"
            android:theme="@style/MainActivityStyle">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".util.permission.PermissionHolderActivity"
            android:configChanges="screenLayout|screenSize|keyboard|keyboardHidden|layoutDirection|orientation|locale|navigation"
            android:screenOrientation="behind"
            android:theme="@style/InsensateActivity" />
        <activity
            android:name=".util.sysResource.SystemPhotographHolderActivity"
            android:configChanges="screenLayout|screenSize|keyboard|keyboardHidden|layoutDirection|orientation|locale|navigation"
            android:screenOrientation="behind"
            android:theme="@style/InsensateActivity" />
        <activity
            android:name=".pages.PureColorPage"
            android:screenOrientation="portrait" />
        <activity
            android:name=".pages.PureColorPage2"
            android:screenOrientation="portrait" />
        <activity
            android:name=".pages.ability.ColorPickerActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".pages.lateral.PureColorPreviewPage"
            android:screenOrientation="portrait" />
        <activity
            android:name=".pages.RotationPage"
            android:screenOrientation="portrait" />
        <activity
            android:name=".pages.FlipPage"
            android:screenOrientation="portrait" />
        <activity
            android:name=".pages.GifPage"
            android:screenOrientation="portrait" />

    </application>

</manifest>